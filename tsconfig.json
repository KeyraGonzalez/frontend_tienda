{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["frontend/src/*"],
      "@/components/*": ["frontend/src/components/*"],
      "@/layouts/*": ["frontend/src/layouts/*"],
      "@/pages/*": ["frontend/src/pages/*"],
      "@/utils/*": ["frontend/src/utils/*"],
      "@/types/*": ["frontend/src/types/*"]
    },
    "types": ["astro/client"]
  }
}
